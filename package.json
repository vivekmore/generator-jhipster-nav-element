    "email": "vivz.thats.me@gmail.com",
    "url": "https://github.com/vivekmore"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivekmore/generator-jhipster-nav-element.git"
  },
  "dependencies": {
    "chalk": "2.1.0",
    "generator-jhipster": "5.0.2",
    "lodash": "4.17.10",
    "mkdirp": "0.5.1",
    "semver": "5.5.0",
    "shelljs": "0.7.8",
    "tabtab": "2.2.2",
    "yeoman-generator": "3.0.0"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "eslint": "^5.0.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "2.13.0",
    "fs": "0.0.1-security",
    "fs-extra": "4.0.2",
    "gulp": "4.0.0",
    "gulp-bump": "2.7.0",
    "gulp-eslint": "4.0.0",
    "gulp-exclude-gitignore": "1.1.1",
    "gulp-git": "2.4.2",
    "gulp-istanbul": "1.1.3",
    "gulp-mocha": "6.0.0",
    "gulp-nsp": "3.0.1",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
