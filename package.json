    "generator",
    "yeoman-generator",
    "jhipster",
    "jhipster-module"
  ],
  "homepage": "https://github.com/vivekmore/generator-jhipster-nav-element",
  "author": {
    "name": "Vivek More",
    "email": "vivz.thats.me@gmail.com",
    "url": "https://github.com/vivekmore"
  },
  "engines": {
    "node": ">=12.15.0",
    "npm": ">=6.13.4"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivekmore/generator-jhipster-nav-element.git"
  },
  "dependencies": {
    "chalk": "3.0.0",
    "generator-jhipster": "6.7.0",
    "lodash": "4.17.15",
    "mkdirp": "1.0.3",
    "semver": "7.1.2",
    "shelljs": "0.8.3",
    "tabtab": "3.0.2",
    "yeoman-generator": "4.5.0"
  },
  "devDependencies": {
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "fs": "0.0.1-security",
    "fs-extra": "8.1.0",
    "istanbul": "0.4.5",
    "mocha": "7.0.1",
    "mocha-lcov-reporter": "1.3.0",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "2.1.0"
  },
  "scripts": {
