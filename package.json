  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivekmore/generator-jhipster-nav-element.git"
  },
  "dependencies": {
    "chalk": "2.4.2",
    "generator-jhipster": "6.0.1",
    "lodash": "4.17.11",
