  },
  "devDependencies": {
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "fs": "0.0.1-security",
    "fs-extra": "7.0.1",
    "istanbul": "0.4.5",
    "mocha": "6.1.4",
